{
  "name": "agentmesh-governance",
  "version": "1.0.0",
  "description": "AgentMesh trust, identity, and governance for multi-agent systems",
  "author": "Agent-Mesh Contributors",
  "license": "MIT",
  "homepage": "https://github.com/imran-siddique/agent-mesh",
  "repository": "https://github.com/imran-siddique/agent-mesh",
  "api_base_url": "https://agentmesh-api.vercel.app",
  "capabilities": [
    "verify_identity",
    "check_trust",
    "enforce_policy",
    "log_audit",
    "get_trust_report"
  ],
  "endpoints": {
    "verify": "POST /api/verify",
    "trust_score": "POST /api/trust/score",
    "policy_evaluate": "POST /api/policy/evaluate",
    "audit_log": "POST /api/audit/log",
    "trust_report": "GET /api/trust/report",
    "health": "GET /api/health",
    "heartbeat": "POST /api/heartbeat"
  },
  "requirements": {
    "agentmesh": ">=1.0.0a3",
    "protocol": "cmvk-v1"
  },
  "heartbeat_interval_seconds": 300,
  "files": {
    "skill": "SKILL.md",
    "heartbeat": "HEARTBEAT.md"
  },
  "tags": [
    "trust",
    "identity",
    "governance",
    "security",
    "multi-agent",
    "cmvk",
    "did"
  ]
}
